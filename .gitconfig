[user]
    email = ryoyukikazu11@gmail.com
    name = Sarada1101
[core]
    editor = vim
    quotepath = false
    excludesfile = /Users/ryo/.gitignore_global
    ignorecase = false
[color]
    diff = auto
    status = auto
    branch = auto
    ui = auto
[alias]
    d = "diff"
    co = "checkout"
    ci = "commit"
    ca = "commit -a"
    ps = "push origin $(git rev-parse --abbrev-ref HEAD)"
    pl = "pull origin $(git rev-parse --abbrev-ref HEAD)"
    st = "status"
    br = "branch"
    ba = "branch -a"
    bm = "branch --merged"
    bn = "branch --no-merged"
[pager]
    log = diff-highlight | less
    show = diff-highlight | less
    diff = diff-highlight | less
[diff]
    tool = vimdiff
[difftool "vimdiff"]
    prompt = false
[merge]
        tool = vscode
[mergetool]
        prompt = false
        keepBackup = false
[mergetool "vscode"]
        # cmd = 'VSCodeのパス' \"$MERGED\"
        # 例：Macの場合
        cmd = '/Applications/Visual Studio Code.app/Contents/Resources/app/bin/code' \"$MERGED\"
        trustExitCode = false